{
  "multipart": [
    {
      "apply": {"model": "vsbrewing:block/brewing_cauldron_stand"},
      "when":  {"is_on_cauldron": "true"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand"},
      "when":  {"is_on_cauldron": "false"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand_bottle0"},
      "when":  {"is_on_cauldron": "false", "has_bottle_0": "true"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand_bottle1"},
      "when":  {"is_on_cauldron": "false", "has_bottle_1": "true"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand_bottle2"},
      "when":  {"is_on_cauldron": "false", "has_bottle_2": "true"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand_empty0"},
      "when":  {"is_on_cauldron": "false", "has_bottle_0": "false"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand_empty1"},
      "when":  {"is_on_cauldron": "false", "has_bottle_1": "false"}
    },
    {
      "apply": {"model": "minecraft:block/brewing_stand_empty2"},
      "when":  {"is_on_cauldron": "false", "has_bottle_2": "false"}
    }
  ]
}
